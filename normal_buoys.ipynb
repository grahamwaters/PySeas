    "import requests\n",
    "import pandas as pd\n",
    "\n",
    "url = 'https://www.ndbc.noaa.gov/data/realtime2/21415.dart'\n",
    "\n",
    "r = requests.get(url)\n",
    "\n",
    "with open('data.csv', 'wb') as f:\n",
    "    f.write(r.content)\n",
    "\n",
    "df = pd.read_csv('data.csv', header=1, parse_dates=True, delimiter = '\\s+')\n",
    "\n",
    "df.head()"
   ]
  }
